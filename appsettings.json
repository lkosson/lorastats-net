{
	"Logging": {
		"LogLevel": {
			"Default": "None"
		},
		"App": {
			"File": "app-{0:yyyyMMdd}.txt",
			"Rotate": 30,
			"LogLevel": {
				"Microsoft.Hosting.Lifetime": "Information",
				"Default": "Warning"
			}
		},
		"Web": {
			"File": "web-{0:yyyyMMdd}.txt",
			"Rotate": 30,
			"LogCategory": false,
			"LogLevel": {
				"LoraStatsNet.Services.RequestLoggingMiddleware": "Information",
				"Default": "None"
			}
		},
		"Console": {
			"LogLevel": {
				"Default": "Information"
			}
		},
		"Debug": {
			"LogLevel": {
				"Default": "Warning"
			}
		}
	},
	"AllowedHosts": "*",
	"DataDir": "data",
	"BlockedIPs": []
}
